{
  "schemaVersion": 1,
  "id": "extshape",
  "version": "${version}",
  "icon": "assets/extshape/icon-fabric.png",
  "name": "Extended Block Shapes",
  "description": "More blocks in more shapes!",
  "authors": [
    "SolidBlock"
  ],
  "contact": {
    "homepage": "https://space.bilibili.com/87328531",
    "sources": "https://github.com/SolidBlock-cn/extshape",
    "issues": "https://github.com/SolidBlock-cn/extshape/issues"
  },
  "license": "LGPLv3",
  "environment": "*",
  "entrypoints": {
    "main": [
      "pers.solid.extshape.ExtShape"
    ],
